    <CSharpProject Include="Gallio.TDNetRunner.Tests\Gallio.TDNetRunner.Tests$(ProjectVariant).csproj" />
  </ItemGroup>

  <ItemGroup>
    <Binary Include="Gallio.TDNetRunner\bin\Gallio.TDNetRunner.dll">
