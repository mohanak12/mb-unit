  <PropertyGroup>
    <MonoCompatible>false</MonoCompatible>
  </PropertyGroup>
  
  <ItemGroup>
    <CSharpProject Include="Gallio.TDNetRunner\Gallio.TDNetRunner$(ProjectVariant).csproj" />
