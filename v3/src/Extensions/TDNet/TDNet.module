    </PluginFile>
    <Binary Include="Gallio.TDNetRunner.UI\bin\Gallio.TDNetRunner.UI.dll">
      <Folder>TDNet</Folder>
    </Binary>
    <PluginFile Include="Gallio.TDNetRunner.UI\Gallio.TDNetRunner.UI.plugin">
      <Folder>TDNet</Folder>
    </PluginFile>

    <Binary Include="Gallio.TDNetRunner.UI\Resources\TestDriven.ico">
